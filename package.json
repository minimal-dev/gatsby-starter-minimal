{
  "name": "gatsby-starter-minimal",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "salexpro <dev@salex.pro>",
  "contributors": [
    {
      "name": "Savelii Lukash",
      "email": "SaveliiLukash@gmail.com"
    }
  ],
  "dependencies": {
    "@gatsbyjs/reach-router": "1.3.6",
    "@percy/cli": "1.0.0-beta.70",
    "@svgr/webpack": "5.5.0",
    "bootstrap": "5.1.3",
    "classnames": "2.3.1",
    "gatsby": "4.1.0",
    "gatsby-plugin-image": "2.1.0",
    "gatsby-plugin-minify-classnames": "0.3.1",
    "gatsby-plugin-offline": "5.1.0",
    "gatsby-plugin-purgecss": "6.1.0",
    "gatsby-plugin-react-helmet": "5.1.0",
    "gatsby-plugin-remove-trailing-slashes": "4.1.0",
    "gatsby-plugin-sass": "5.1.0",
    "gatsby-plugin-sharp": "4.1.0",
    "gatsby-plugin-svgr-svgo": "1.2.2",
    "gatsby-source-filesystem": "4.1.0",
    "gatsby-transformer-sharp": "4.1.0",
    "gsap": "3.8.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-bootstrap": "2.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "sass": "1.43.4"
  },
  "devDependencies": {
    "@commitlint/cli": "14.1.0",
    "@commitlint/config-conventional": "14.1.0",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "1.1.25",
    "husky": "7.0.4",
    "lint-staged": "11.2.6",
    "prettier": "2.4.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -H 0.0.0.0 -o",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{js,jsx,ts,tsx,json,mdx,md,css,scss}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "prepare": "husky install",
    "cz": "cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "eslint"
    ],
    "*.{css,scss,md,mdx}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
